<DayButton>:
	canvas.after:
        Color:
            rgba: .5, .5, .5, 1
        Line:
            width: 1
            rectangle: self.x, self.y, self.width, self.height
    group: "day_num"
	theme_text_color: 'Custom'
	halign: "center"
	size_hint: 0.1, 0.1
	#pos_hint: {'top': 1}
	#size_hint_x: None
	#width: 200
